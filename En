local AutoLoadMap_Button = Create_Toggle(ModeAuto_SelectFuntion, "ออโต้โหลดแมพ 🌍")
local AutoLoadMap = false;AutoLoadMap_Button.Activated:Connect(function()
	if AutoLoadMap == false then
		AutoLoadMap = true
	else
		AutoLoadMap = false
		for i=1,5 do
			wait()
			HMNRT.CFrame = InnerTouchZone.CFrame * CFrame.new(0,7,0)
		end
	end	
end)
local orbitTime = 0
local orbitRadius = 10
local orbitSpeed = 50
RunService.RenderStepped:Connect(function(dt)
	if AutoLoadMap == true then
		pcall(function()
			if not InnerTouchZone then return end
			orbitTime = orbitTime + dt
			local angle = math.rad(orbitSpeed) * orbitTime
			local offset = Vector3.new(math.cos(angle) * orbitRadius, 40, math.sin(angle) * orbitRadius)
			local newPos = InnerTouchZone.Position + offset
			if HMNRT and newPos and orbitRadius and orbitSpeed then
				HMNRT.CFrame = CFrame.new(newPos)
				if orbitRadius < 1200 then
					orbitRadius = orbitRadius + 0.5
					if orbitRadius >= 800 then
						orbitSpeed = 25
					end
				else
					orbitRadius = 10
					orbitSpeed = 50
				end
			end
		end)
		if orbitRadius >= 1200 and AutoLoadMap == true then
			for i=1,5 do
				wait()
				HMNRT.CFrame = InnerTouchZone.CFrame * CFrame.new(0,7,0)
			end
			repeat
				wait(0.1)
			until AutoLoadMap == false
		end
	end
end)

AutoPickGoldCoin_Button = Create_Toggle(ModeFarm_SelectFuntion, "ออโต้เก็บเหรียญทอง 💰 ")
local AutoPickGoldCoin = false;AutoPickGoldCoin_Button.Activated:Connect(function()
	if AutoPickGoldCoin == false then
		AutoPickGoldCoin = true
	else
		AutoPickGoldCoin = false
	end
end)
AutoPickFlower_Button = Create_Toggle(ModeFarm_SelectFuntion, "ออโต้เก็บดอกไม้ 💐 ")
local AutoPickFlower = false;AutoPickFlower_Button.Activated:Connect(function()
	if AutoPickFlower == false then
		AutoPickFlower = true
	else
		AutoPickFlower = false
	end
end)
task.spawn(function()
	local RequestCollectCoints = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):FindFirstChild("RequestCollectCoints")
	local RequestPickFlower = game:GetService("ReplicatedStorage"):WaitForChild("RemoteEvents"):FindFirstChild("RequestPickFlower")
	while wait(5) do
		if AutoPickGoldCoin == true then
			for _, item2 in ipairs(Items:GetChildren()) do
				if item2.Name == "Coin Stack" then
					RequestCollectCoints:InvokeServer(item2)
				end
				wait()
			end
		end
		if AutoPickFlower == true then
			for _, item3 in ipairs(Landmarks:GetChildren()) do
				if item3.Name == "Flower" then
					RequestPickFlower:InvokeServer(item3)
				end
				wait()
			end
		end
	end
end)

