local Kill_Aura_Dis = 100 ; local Max_Kill_Aura_Dis = 200
local Kill_Aura_SlideBar, Kill_Aura_Bar, Kill_Aura_TextBox, Kill_Aura_ShowValue, Kill_Aura_Color, Kill_Aura_ResetButton = Create_Slide_Textbox(ModeFarm_SelectFuntion, "ระยะการคิลออร่า", "ใส่ค่าที่ต้องการเลยครับ", Kill_Aura_Dis)
local function SetSlider5(Bar, Slide, Defalse_Value, Color)
	local slider_Kill_Aura = createSlider(Bar, Slide, function(value)
		Kill_Aura_Dis = value;	Kill_Aura_TextBox.Text = ""
	end, 200)
	slider_Kill_Aura.SetValue(Kill_Aura_Dis)
	Kill_Aura_TextBox.FocusLost:Connect(function(enterPressed)
		local text = Kill_Aura_TextBox.Text
		local number = tonumber(text)
		if number then
			Kill_Aura_Dis = number
			slider_Kill_Aura.SetValue(number)
		else
			Kill_Aura_TextBox.Text = ""
		end
	end)
	Kill_Aura_ResetButton.Activated:Connect(function()
		Kill_Aura_Dis = 1
		slider_Kill_Aura.SetValue(1)
	end)
	task.spawn(function()
		while wait(0.3) do
			Kill_Aura_ShowValue.Text = Kill_Aura_Dis
			if tonumber(Kill_Aura_ShowValue.Text) == Max_Kill_Aura_Dis then
				tweenButtonColor(Color, Color3.new(0.145098, 0.133333, 0.392157), 0.17)
			else
				tweenButtonColor(Color, Color3.new(0.45098, 0, 1), 0.17)
			end
		end
	end)
end
