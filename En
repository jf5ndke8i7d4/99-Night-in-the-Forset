ModeMoreLine_1 = Create_Line(ModeMore_SelectFuntion, "การมองเห็น 👁️")

EspChest_Button = Create_Toggle(ModeMore_SelectFuntion, "มองทะลุหีบ")
local EspChest = false;EspChest_Button.Activated:Connect(function()
	if EspChest == false then
		EspChest = true
	else
		EspChest = false
		for _, item2 in ipairs(Items:GetChildren()) do
			if item2 and matchesAllowedKeyword(item2.Name, {"Chest"}) and item2:IsA("Model") then
				if item2:FindFirstChild("SOmtankChestESP") then
					item2:FindFirstChild("SOmtankChestESP"):Destroy()
				end
				if item2:FindFirstChild("SOmtankChestBil") then
					item2:FindFirstChild("SOmtankChestBil"):Destroy()
				end
			end
			wait()
		end
	end	
end)
task.spawn(function()
	while wait(6) do
		if EspChest == true then
			for _, item2 in ipairs(Items:GetChildren()) do
				if item2 and matchesAllowedKeyword(item2.Name, {"Chest"}) and item2:IsA("Model") then
					if not item2:FindFirstChild("SOmtankChestESP") then
						local NewHi = Instance.new("Highlight", item2)
						local NewBil = Instance.new("BillboardGui", item2)
						local NewText = Instance.new("TextLabel", NewBil)
						NewHi.Name = "SOmtankChestESP"
						NewBil.Name = "SOmtankChestBil"
						NewHi.OutlineTransparency = 1
						NewHi.FillColor = Color3.new(0.407843, 0.352941, 1)
						NewText.BackgroundTransparency = 1
						NewText.TextColor3 = Color3.new(0.415686, 0.184314, 1)
						NewText.Text = "Chest"
						NewBil.Size = UDim2.new(0, 200,0, 50)
						NewBil.SizeOffset = Vector2.new(0, 0.5)
						NewBil.AlwaysOnTop = true
						NewText.Size = UDim2.new(0, 200,0, 50)
						NewText.TextYAlignment = Enum.TextYAlignment.Top
						NewText.TextSize = 26
						NewText.Font = Enum.Font.Highway
						pcall(function()
							repeat
								wait(0.01)
							until item2.PrimaryPart.ProximityAttachment.ProximityInteraction
							local Prox = item2.PrimaryPart.ProximityAttachment.ProximityInteraction
							Prox.Triggered:Connect(function()
								if item2:FindFirstChild("SOmtankChestBil") and item2:FindFirstChild("SOmtankChestBil"):FindFirstChild("TextLabel") then
									item2:FindFirstChild("SOmtankChestBil"):FindFirstChild("TextLabel").Text = "Chest - Opened"
								end
							end)
						end)
					end
				end
				wait()
			end
		end	
	end
end)

ModeMoreLine_2 = Create_Line(ModeMore_SelectFuntion, "ช่วยลดความแล็ค 🏞️")

NoLeaf_Button = Create_Toggle(ModeMore_SelectFuntion, "ลบสิ่งไม่ไม่จำเป็น 🍃")
local NoLeaf = false;NoLeaf_Button.Activated:Connect(function()
	if NoLeaf == false then
		NoLeaf = true
	else
		NoLeaf = false
	end	
end)
task.spawn(function()
	local function SoftDestroyLeaf(Model)
		if Model then
			for _, item in ipairs(Model:GetChildren()) do
				if item:IsA("BasePart") and item.Material == Enum.Material.Grass then
					item:Destroy()
				end						
			end
		end
	end
	while wait(10) do
		if NoLeaf == true then
			for _, item in ipairs(Map.Ground:GetChildren()) do
				if item:IsA("BasePart") and item.Material == Enum.Material.Grass then
					item.Material = Enum.Material.SmoothPlastic
					item.CastShadow = false
				end	
				wait()
			end
			for _, item in ipairs(Foliage:GetChildren()) do
				pcall(function()
					if NoLeaf == true then
						if item.Name == "Bush" then
							item:Destroy()
						elseif item.Name == "StoneTall" or item.Name == "StoneSmall" then
							item:Destroy()
						elseif item.Name == "Grass1" or item.Name == "Grass2" then
							item:Destroy()
						elseif item.Name == "Small Tree" then
							SoftDestroyLeaf(item)
						elseif item.Name == "TreeBig1" or item.Name == "TreeBig2" or item.Name == "TreeBig3" then
							SoftDestroyLeaf(item)
						end
					end
				end)
				wait()
			end
			for _, item in ipairs(Landmarks:GetChildren()) do
				pcall(function()
					if NoLeaf == true then
						if item.Name == "Small Tree" then
							SoftDestroyLeaf(item)
						end
					end
				end)
				wait()
			end	
		end	
	end
end)


