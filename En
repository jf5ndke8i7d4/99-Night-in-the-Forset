local SetScriptTp_StrongHold = false; local CheckDaleyStrongHoldTpButton = 1
local TeleStrongHold_Button, TeleTextMain_Button, TeleMain_Button = Create_Click(ModeTeleport_SelectFuntion, "สตรองโฮลด์ 💎")
local NotThisNameTele = {"Spawn", "No Respawn", "Bush", "Burrow", "Portion", "Stack", "Flower", "Hollow", "Mud", "Grave", "Pillars", "Trap", "Stump", "Small Tree", "Wide", "Hole", "Well"}
Landmarks = Map:WaitForChild("Landmarks")
local function ResetAllStucInServer()
	for _, item in ipairs(ModeTeleport_SelectFuntion:GetChildren()) do
		if item:IsA("Frame") and matchesAllowedKeyword(item.Name, {"Reset_"}) then
			item:Destroy()
		end
		wait()
	end
	for _, item in ipairs(Landmarks:GetChildren()) do
		if matchesAllowedKeyword(item.Name, NotThisNameTele) == false then
			if not ModeTeleport_SelectFuntion:FindFirstChild("Reset_"..item.Name) then
				local NewBring_Button = Create_Click(ModeTeleport_SelectFuntion, item.Name, "Reset_")
				NewBring_Button.Activated:Connect(function()
					if Landmarks:FindFirstChild(item.Name) then
						local Terget_Stuc = Landmarks:FindFirstChild(item.Name)
						if Terget_Stuc and Terget_Stuc.PrimaryPart then
							HMNRT.CFrame = item.PrimaryPart.CFrame * CFrame.new(0,20,0)
						end
					end
				end)
			end
			wait()
		end
	end
end
ModeTeleport.Activated:Connect(function()
	ResetAllStucInServer()
end)

