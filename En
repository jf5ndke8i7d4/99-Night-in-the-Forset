local FastBring_Delay = 0.12
local ModeDragLine_N1 = Create_Line(ModeDrag_SelectFuntion, "เพิ่มเติม ⚙️")
local FastBring_Button = Create_Toggle(ModeDrag_SelectFuntion, "ดึงแบบเร็ว")
local FastBring = false;FastBring_Button.Activated:Connect(function()
	if FastBring == true then
		FastBring = false
		FastBring_Delay = 0.12
	else
		FastBring = true
		FastBring_Delay = 0.06
	end
end)
local Bring_Count = 100 ; local Max_Bring_Count = 100
local Bring_Count_SlideBar, Bring_Count_Bar, Bring_Count_TextBox, Bring_Count_ShowValue, Bring_Count_Color, Bring_Count_ResetButton = Create_Slide_Textbox(ModeDrag_SelectFuntion, "จำนวนที่ต้องการดึง", "ใส่ค่าที่ต้องการเลยครับ", Bring_Count)
local function SetSlider4(Bar, Slide, Defalse_Value, Color)
	local slider_Bring_Count = createSlider(Bar, Slide, function(value)
		Bring_Count = value;	Bring_Count_TextBox.Text = ""
	end, 100)
	slider_Bring_Count.SetValue(Bring_Count)
	Bring_Count_TextBox.FocusLost:Connect(function(enterPressed)
		local text = Bring_Count_TextBox.Text
		local number = tonumber(text)
		if number then
			Bring_Count = number
			slider_Bring_Count.SetValue(number)
		else
			Bring_Count_TextBox.Text = ""
		end
	end)
	Bring_Count_ResetButton.Activated:Connect(function()
		Bring_Count = 1
		slider_Bring_Count.SetValue(1)
	end)
	task.spawn(function()
		while wait(0.1) do
			Bring_Count_ShowValue.Text = Bring_Count
			if tonumber(Bring_Count_ShowValue.Text) == Max_Bring_Count then
				tweenButtonColor(Color, Color3.new(0.145098, 0.133333, 0.392157), 0.17)
			else
				tweenButtonColor(Color, Color3.new(0.45098, 0, 1), 0.17)
			end
		end
	end)
end
SetSlider4(Bring_Count_SlideBar, Bring_Count_Bar, Bring_Count, Bring_Count_Color)
local OldCFrame_Bring = HMNRT.CFrame
local function BringItemWithItem2(item2)
	pcall(function()
		OldCFrame_Bring = HMNRT.CFrame
		HMNRT.CFrame = item2.PrimaryPart.CFrame
		wait(FastBring_Delay)
		if item2.Name == "Berry" then
			for i=1,5 do
				pcall(function()
					RequestStartDraggingItem:FireServer(item2)
					wait(FastBring_Delay)
				end)
			end
		else
			RequestStartDraggingItem:FireServer(item2)
		end
		if Target == "Character" then
			wait(FastBring_Delay)
			item2.PrimaryPart.CFrame = HMNRT.CFrame * CFrame.new(math.random(-3,3),2,-3 + math.random(-3,3))
		elseif Target == "Craft" then
			wait(FastBring_Delay)
			item2.PrimaryPart.CFrame = DashedLine.CFrame * CFrame.new(0,7,0)
		elseif Target == "NearCamp" then
			wait(FastBring_Delay)
			item2.PrimaryPart.CFrame = InnerTouchZone.CFrame * CFrame.new(-20,2,0)
		elseif Target == "Fire" then
			wait(FastBring_Delay)
			item2.PrimaryPart.CFrame = InnerTouchZone.CFrame * CFrame.new(0,15,0)
		end
		HMNRT.CFrame = OldCFrame_Bring
		wait(FastBring_Delay)
		StopDraggingItem:FireServer(item2)
		wait(FastBring_Delay)
		StopDraggingItem:FireServer(item2)
	end)
end
local Item_Fuel_Teble = {"Sapling", "Chair", "Coal", "Fuel Canister", "Log", "Oil Barrel"}
local Item_Weapon_Teble = {"Bandage", "Iron Body", "Leather Body", "MedKit", "Revolver", "Revolver Ammo", "Rifle", "Rifle Ammo", "Strong Axe", "Laser Sword", "Raygun"}
local Item_Food_Teble = {"Apple", "Berry", "Cake", "Carrot", "Meat? Sandwich"}
local Item_Scrap_Teble = {"Broken Microwave", "Metal Chair", "Old Car Engine", "Old Radio", "Sheet Metal", "Tyre", "UFO Junk", "UFO Scrap", "Washing Machine"}
local ModeDragLine_2 = Create_Line(ModeDrag_SelectFuntion, "อาหาร 🍇")
local Drop_Food_Item, Drop_Food_Item_List, ResetButton_Bring_Food_Item = Create_SelectPick(ModeDrag_SelectFuntion, "เลือกไอเท็ม", 5)
ResetButton_Bring_Food_Item.Visible = false
for _, Item_In_Game in ipairs(Item_Food_Teble) do
	local NewButton = Create_Toggle_InSC(Drop_Food_Item, Item_In_Game)
	NewButton.Activated:Connect(function()
		if Drop_Food_Item_List:FindFirstChild(Item_In_Game) then
			Drop_Food_Item_List:FindFirstChild(Item_In_Game):Destroy()
		else
			local NewTag = Instance.new("RayValue", Drop_Food_Item_List) ; NewTag.Name = Item_In_Game
		end
	end)
	wait()
end
local Bring_Food_Button, Bring_Food_Button_Text = Create_Click(ModeDrag_SelectFuntion, "ดึงอาหารที่เลือก")
Bring_Food_Button.Activated:Connect(function()
	Bring_Food_Button_Text.Text = "กำลังดึงอาหารที่เลือก"
	Bring_Food_Button.Active = false
	for _, item1 in ipairs(Drop_Food_Item_List:GetChildren()) do
		if item1:IsA("RayValue") then
			local TargetName = item1.Name
			local Bring_Count_Work = 0
			for _, item2 in ipairs(Items:GetChildren()) do
				if item2.Name == TargetName and item2.PrimaryPart then
					if Bring_Count_Work < Bring_Count then
						BringItemWithItem2(item2)
						Bring_Count_Work = Bring_Count_Work + 1
					end
				end
				if FastBring == false then
					wait()
				end
			end
			HMNRT.CFrame = OldCFrame_Bring
		end
	end
	HMNRT.CFrame = OldCFrame_Bring
	Bring_Food_Button_Text.Text = "ดึงอาหารที่เลือก"
	Bring_Food_Button.Active = true
end)

